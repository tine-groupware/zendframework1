{
  "name": "zendframework/zendframework1",
  "description": "Metaways Infosystems GmbH - Patched Zendframework1",
  "type": "project",
  "license" : ["New BSD License"],
  "require": {
    "php": ">=8.2"
  },
  "autoload": {
    "psr-0": {
      "Zend_": ["library/"]
    },
    "files": ["library/bootstrap.php"]
  },
  "config": {
    "vendor-dir": "."
  },
  "require-dev": {
    "rector/rector": "^2.3"
  },
  "scripts": {
    "rector:test": "bin/rector process --config .rector.php --dry-run",
    "rector:fix": "bin/rector process --config .rector.php"
  }
}
